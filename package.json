{
  "name": "@nosto/search-js",
  "version": "3.1.1",
  "license": "ISC",
  "type": "module",
  "files": [
    "dist"
  ],
  "workspaces": [
    "packages/*"
  ],
  "main": "./dist/core/search.cjs.js",
  "module": "./dist/core/search.es.js",
  "types": "./dist/core/src/search.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "exports": {
    ".": {
      "types": "./dist/core/core.d.ts",
      "import": "./dist/core/core.es.js",
      "require": "./dist/core/core.cjs.js"
    },
    "./currencies": {
      "types": "./dist/currencies/currencies.d.ts",
      "import": "./dist/currencies/currencies.es.js",
      "require": "./dist/currencies/currencies.cjs.js"
    },
    "./thumbnails": {
      "types": "./dist/thumbnails/thumbnails.d.ts",
      "import": "./dist/thumbnails/thumbnails.es.js",
      "require": "./dist/thumbnails/thumbnails.cjs.js"
    },
    "./preact/legacy": {
      "types": "./dist/preact/legacy.d.ts",
      "import": "./dist/preact/legacy.es.js",
      "require": "./dist/preact/legacy.cjs.js"
    },
    "./utils": {
      "types": "./dist/utils/utils.d.ts",
      "import": "./dist/utils/utils.es.js",
      "require": "./dist/utils/utils.cjs.js"
    },
    "./preact/common": {
      "types": "./dist/preact/common.d.ts",
      "import": "./dist/preact/common.es.js",
      "require": "./dist/preact/common.cjs.js"
    },
    "./preact/serp": {
      "types": "./dist/preact/serp.d.ts",
      "import": "./dist/preact/serp.es.js",
      "require": "./dist/preact/serp.cjs.js"
    },
    "./preact/category": {
      "types": "./dist/preact/category.d.ts",
      "import": "./dist/preact/category.es.js",
      "require": "./dist/preact/category.cjs.js"
    },
    "./preact/autocomplete": {
      "types": "./dist/preact/autocomplete.d.ts",
      "import": "./dist/preact/autocomplete.es.js",
      "require": "./dist/preact/autocomplete.cjs.js"
    },
    "./preact/hooks": {
      "types": "./dist/preact/hooks.d.ts",
      "import": "./dist/preact/hooks.es.js",
      "require": "./dist/preact/hooks.cjs.js"
    }
  },
  "keywords": [],
  "author": "Nosto",
  "scripts": {
    "build": "tsc && vite build && npm run typedoc",
    "dev": "concurrently \"npm run dev:library\" \"npm run dev:preact\"",
    "dev:library": "vite build --watch",
    "dev:preact": "cd dev/preact && npm run dev",
    "typedoc": "typedoc --treatWarningsAsErrors",
    "test": "vitest --run --coverage && (cd dev/preact && npm run test)",
    "test:watch": "vitest",
    "lint": "eslint",
    "lint-fix": "eslint --fix",
    "install:dev": "(cd dev/preact && npm i --ignore-scripts)",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nosto/nosto-js": "^2.3.0",
    "@testing-library/dom": "^10.4.0",
    "@types/dom-speech-recognition": "^0.0.6",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^24.0.7",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-barrel-files": "^3.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "isbot": "^5.1.28",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
